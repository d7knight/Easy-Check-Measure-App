<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.david.easycheckmeasureapp" >
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        tools:replace="android:icon,android:theme" >
        <activity
            android:name=".CreateNewCheckMeasureActivity"
            android:label="@string/app_name" >
            ndroid:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation"
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ViewOldCheckMeasuresActivity"
            android:label="@string/title_activity_view_old_check_measures"
            android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation">
        </activity>

        <activity
            android:name=".SendCheckMeasureActivity"
            android:label="@string/title_activity_send_check_measure"
            android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation">
        </activity>
    </application>

</manifest>
